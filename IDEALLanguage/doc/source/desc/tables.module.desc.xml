			<element name="label" docName="label-content-model"/>
			<element name="summary" docName="summary-content-model"/>
			<element name="col" docName="col-content-model"/>
			<element name="thead" docName="thead-content-model"/>
			<element name="tbody" docName="tbody-content-model"/>
			<element name="tfoot" docName="tfoot-content-model"/>
			<element name="tr" docName="tr-content-model"/>
			<element name="td" docName="td-content-model"/>
			<element name="th" docName="th-content-model"/>
		</elements>		
	</module>
</module_desc>